Global="*res://globals/global.tscn"
State="*res://globals/state.tscn"
BgmPlayer="*res://globals/bgm_player.tscn"
Main="*res://globals/main.tscn"
Game="*res://globals/game.tscn"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"boss": "res://dialogic/characters/boss.dch",
"player": "res://dialogic/characters/player.dch"
}
directories/dtl_directory={
"end": "res://dialogic/timelines/end.dtl",
"lv0": "res://dialogic/timelines/lv0.dtl",
"lv1": "res://dialogic/timelines/lv1.dtl",
"lv2": "res://dialogic/timelines/lv2.dtl",
"lv3": "res://dialogic/timelines/lv3.dtl",
"lv4": "res://dialogic/timelines/lv4.dtl",
"lv5": "res://dialogic/timelines/lv5.dtl",
"lv6": "res://dialogic/timelines/lv6.dtl"
}
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0
text/autoadvance_ignored_characters_enabled=true
audio/max_channels=4.0
animations/join_default_length=0.5
animations/join_default_wait=false
animations/leave_default_length=0.5
animations/leave_default_wait=false
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
layout/style_list=["res://dialogic/styles/visual_novel_style.tres"]
layout/default_style=""
glossary/default_case_sensitive=true
variables={}
translation/enabled=false
translation/original_locale="en"
translation/file_mode=1
translation/translation_folder=""
translation/save_mode=0
translation/add_separator=false
text/autoadvance_fixed_delay=0.0
text/autoadvance_enabled=false
text/autoadvance_per_word_delay=0
history/simple_history_enabled=false

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="uid://1kkcqbwfxe3y"
mouse_cursor/custom_image_hotspot=Vector2(64, 64)

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

[global_group]

player=""
bullet=""

